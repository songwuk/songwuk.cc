{
  "name": "astrojs-i18n",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@7.12.0",
  "description": "",
  "author": "Song wuk <1017301325@qq.com>",
  "license": "MIT",
  "homepage": "https://github.com/songwuk/songwuk.cc#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/songwuk/songwuk.cc.git"
  },
  "bugs": "https://github.com/songwuk/songwuk.cc/issues",
  "keywords": [
    "internationalization",
    "i18n",
    "i18next",
    "astrojs"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "default": "./dist/components/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "index": [
        "./dist/*",
        "./dist/index.d.ts"
      ],
      "components": [
        "./dist/components/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "i18next": "^21.9.2",
    "@proload/core": "^0.3.3",
    "fast-glob": "^3.2.12"
  },
  "devDependencies": {
    "@types/node": "^18.7.5",
    "astro": "^1.3.0",
    "bumpp": "^8.2.1",
    "eslint": "^8.23.1",
    "esno": "^0.16.3",
    "pnpm": "^7.12.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.8.3",
    "unbuild": "^0.8.11",
    "vite": "^3.1.2",
    "vitest": "^0.23.4"
  }
}
