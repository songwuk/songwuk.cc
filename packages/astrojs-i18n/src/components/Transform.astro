---
import { transformHtml } from "./utils";
export interface Props {
  i18nKey: string,
  ns: string
}
const { i18nKey, ns } = Astro.props;
let html: string = '';
if (Astro.slots.has('default')) {
  html = await Astro.slots.render('default')
}
---
<!-- Fragments can also be useful to avoid wrapper elements when adding set:* directives, as in the following example: -->
<!-- https://docs.astro.build/en/core-concepts/astro-components/#fragments--multiple-elements -->

<Fragment set:html={transformHtml(html, i18nKey, ns)} />